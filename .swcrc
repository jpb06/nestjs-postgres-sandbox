{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": true
    },
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true
    },
    "target": "ES2020",
    "keepClassNames": true
  },
  "module": {
    "type": "commonjs",
    "noInterop": false
  }
}
