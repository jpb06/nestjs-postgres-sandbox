{
  "name": "nestjs-prisma-postgres-sandbox",
  "version": "0.0.1",
  "description": "Toying with nestjs",
  "author": "jpb06",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": "16.x",
    "yarn": "1.x"
  },
  "scripts": {
    "ts-node": "ts-node --compiler-options {\\\"module\\\":\\\"commonjs\\\"}",
    "prebuild": "rimraf dist",
    "build": "yarn prisma:merge && yarn prisma:gen && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "dev:db": "yarn prisma:merge && yarn prisma:gen && yarn prisma:seed",
    "docker": "docker-compose down && docker-compose up sandbox-postgres",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:leak": "yarn jest --max-workers=1 --no-cache",
    "test:watch": "jest --watch",
    "test:dev": "jest --watchAll --coverage",
    "test:e2e": "jest --watchAll --coverage .spec.ts",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:ci": "node --expose-gc ./node_modules/.bin/jest . --coverage --no-cache",
    "prisma:merge": "ts-node ./scripts/merge.prisma.schemas.ts && yarn prisma format --schema prisma/schema.prisma",
    "prisma:gen": "prisma generate",
    "prisma:seed": "yarn prisma migrate dev && yarn prisma db seed"
  },
  "dependencies": {
    "@nestjs/common": "^8.4.4",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.4.4",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.4.4",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/terminus": "^8.0.6",
    "@prisma/client": "^3.12.0",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "swagger-ui-express": "^4.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.5",
    "@nestjs/schematics": "^8.0.10",
    "@nestjs/testing": "^8.4.4",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.25",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-prettier": "^4.0.0",
    "fs-extra": "^10.1.0",
    "jest": "^27.5.1",
    "jest-mock-extended": "^2.0.5",
    "jsonwebtoken": "^8.5.1",
    "prettier": "^2.6.2",
    "prisma": "^3.12.0",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
