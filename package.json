{
  "name": "nestjs-prisma-postgres-sandbox",
  "version": "0.0.1",
  "description": "Toying with nestjs",
  "author": "jpb06",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": "14.x",
    "yarn": "1.22.10"
  },
  "scripts": {
    "ts-node": "ts-node --compiler-options {\\\"module\\\":\\\"commonjs\\\"}",
    "prebuild": "rimraf dist",
    "build": "yarn prisma:merge && yarn prisma:gen && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "dev:db": "docker-compose down && docker-compose up sandbox-postgres",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:dev": "jest --watchAll --coverage",
    "test:e2e": "jest --watchAll --coverage .spec.ts",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:ci": "jest --ci --coverage",
    "prisma:merge": "ts-node ./scripts/merge.prisma.schemas.ts && yarn prisma format --schema prisma/schema.prisma",
    "prisma:gen": "prisma generate",
    "prisma:seed": "yarn prisma migrate dev && yarn prisma db seed --preview-feature"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.18",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.18",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.6",
    "@nestjs/platform-express": "^7.6.18",
    "@nestjs/swagger": "^4.8.2",
    "@nestjs/terminus": "^7.1.2",
    "@prisma/client": "^2.26.0",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.1.0",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^7.6.18",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.12",
    "@types/fs-extra": "^9.0.11",
    "@types/jest": "^26.0.22",
    "@types/jsonwebtoken": "^8.5.3",
    "@types/node": "^15.12.3",
    "@types/passport-jwt": "^3.0.5",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.3.1",
    "fs-extra": "^10.0.0",
    "jest": "^27.0.6",
    "jest-mock-extended": "^1.0.16",
    "jsonwebtoken": "^8.5.1",
    "prettier": "^2.3.2",
    "prisma": "^2.26.0",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.5"
  }
}